$TTL    604800
@       IN      SOA     ns1.ab-finance.no. root.ab-finance.no. (
                              5         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;

; Name servers
ab-finance.no.  IN      NS      ns1.ab-finance.no.

; A records for name servers
ns1             IN      A       <DNS_SERVER>

; Other A records
work            IN      A       <EMPLOYEE_SERVER>
gitlab          IN      A       <GITLAB_SERVER>
www.work        IN      CNAME   work
www.gitlab      IN      CNAME   gitlab